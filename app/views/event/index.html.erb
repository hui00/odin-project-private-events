<div>
  <h1 class="font-bold text-4xl">Event#index</h1>
  <p>Find me in app/views/event/index.html.erb</p>
  <% unless @events.empty? %>
    <% @event.attributes.each do |attr_name, attr_value| %>
      <div class="work_profile_attribute">
        <%= attr_name %>: <%= attr_value %>
      </div>
    <% end %>
  <% end %>
</div>
