<div>
  <div class="mb-8">
    <h1 class="font-bold text-4xl">Users#show</h1>
    <p>Find me in app/views/users/index.html.erb</p>
  </div>
  <div>
    <p class="text-xl mb-2">User:  <%= @user.email %> </p>
  <h2 class="font-semibold text-2xl mb-2"> Events: </h2>
  </div>
  <% @user.events.each do |event| %>
    <h3 class="text-3xl"> <%= event.name %> </h3>
    <p class="mb-4"> <%= event.date %> </p>
  <% end %>
</div>
